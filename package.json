{
  "name": "pgwebia-backend",
  "version": "1.0.0",
  "description": "Backend do PGWebIA rodando Fastify, Prisma e TypeScript com aliases prontos",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "generate": "prisma generate",
    "start": "npm run generate && npm run build && node dist/server-fastify.js",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "fastify",
    "prisma",
    "typescript",
    "pgwebia"
  ],
  "author": "Elton",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/express": "^4.0.2",
    "@fastify/jwt": "^9.1.0",
    "@prisma/client": "^6.10.1",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.6.1",
    "fastify": "^5.4.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.12.7",
    "prisma": "^6.10.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
